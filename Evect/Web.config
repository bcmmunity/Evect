<?xml version="1.0" encoding="utf-8"?>
<configuration>
<location path="." inheritInChildApplications="false" allowOverride="false">
    <system.webServer>

        <handlers> <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" /> </handlers>
        <aspNetCore processPath="dotnet" arguments=".\Evect.dll" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" hostingModel="InProcess" />
    </system.webServer>
    <system.web>
        <trust level="Full" />
    </system.web>
</location>
<system.web>
    <compilation defaultLanguage="c#" />
</system.web>
</configuration>
